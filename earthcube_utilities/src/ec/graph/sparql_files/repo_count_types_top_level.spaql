# returns count of top level types for repo

prefix schema: <https://schema.org/>
SELECT  ?type  (count(distinct ?s ) as ?scount)
WHERE {
Graph ?g {

       ?s a ?type .
FILTER ( !isBLANK(?s) )  .
FILTER( STRSTARTS(str(?g), "urn:gleaner:summoned:${repo}"))
       }
}

GROUP By ?type
ORDER By DESC(?scount)
